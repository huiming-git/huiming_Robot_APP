CompileFlags:
  Add:
    - '-ferror-limit=0'
    - '-Wno-implicit-int'
  CompilationDatabase: out/build/CPP
  Remove:
    - '-fmodules-ts'
    - '-fmodule-mapper=*'
    - '-fdeps-format=*'
Diagnostics:
  Suppress:
    - unused-includes
    - unknown_typename
    - unknown_typename_suggest
    - typename_requires_specqual
